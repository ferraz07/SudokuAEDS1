void desenha_grade() {
  int i,j;
  printf("   1  2  3   4  5  6   7  8  9   \n");
  printf(" +---------+---------+---------+ \n");
  for (i = 0; i < N; i++) {
    printf("%d", i+1);
	printf("|");
	for (j = 0; j < N; j++) {
	  if (principal[i][j] != 0)
	printf(" %d ", principal[i][j]);
	  else
	printf("   ");
	  if (j % 3 == 2)
	printf("|");
	}
	if (i % 3 == 2)
	  printf("\n +---------+---------+---------+ ");
	printf("\n");
  }
}
